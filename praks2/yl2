#!/bin/bash
#
# skript 2 oma andmetega
#
# skripti algus
who | cut -f1 -d" " | cut -f2 -d"." > perenimi #l천ikab perenime mis on 2-ne peale punkti ja salvestab
who | cut -f1 -d" " | cut -f1 -d"." > eesnimi #l천ikab eesnime ja salvestab
paste -d"." perenimi eesnimi > nimed #kleebivad kokku
cat nimed | sort | uniq # sorteerib v채ljundi ja korduvad eemaldab
#
# skripti l천pp

