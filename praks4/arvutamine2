#!/bin/bash
#skripti algus
echo -n "Mitu inimest on teie grupis?: "
read inimesed #kysib sisendit
echo -n "Mitu kohta on yhes bussis?: "
read kohad #kysib sisendit
ahah1=`expr $inimesed / $kohad` #arvutab
ahah2=`expr $inimesed % $kohad` #arvutab
echo "bussis/bussides on $ahah1 t채idetud kohta ja maha j채채b $ahah2 inimest"
#skripti l천pp
